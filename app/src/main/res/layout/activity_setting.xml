        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/View_Deduct_Rice_PP"
        app:layout_constraintVertical_bias="0.5" />

    <EditText
        android:id="@+id/Edit_Deduct_Wheat_PP"
        android:layout_width="87dp"
        android:layout_height="55dp"
        android:ems="10"
        android:hint="Kg"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/Edit_Free_Rice_PP"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/View_Deduct_Wheat_PP"
