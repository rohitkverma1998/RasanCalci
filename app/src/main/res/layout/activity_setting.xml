<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Setting">

    <TextView
        android:id="@+id/View_Deduct_Rice_PP"
        android:layout_width="229dp"
        android:layout_height="56dp"
        android:text="Deduct Rice/Person"
        app:layout_constraintBottom_toTopOf="@+id/View_Deduct_Wheat_PP"
        app:layout_constraintEnd_toStartOf="@+id/Edit_Deduct_Rice_PP"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/View_Wheat_PP"
        app:layout_constraintVertical_bias="0.5" />

    <TextView
        android:id="@+id/View_Deduct_Wheat_PP"
        android:layout_width="229dp"
        android:layout_height="56dp"
        android:text="Deduct Wheat/Person"
